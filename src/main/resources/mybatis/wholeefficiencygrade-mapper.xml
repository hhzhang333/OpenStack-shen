<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.seu.openstack.dao.WholeEfficiencyGradeDao">
    <select id="selectEfficiencyGrade" resultType="cn.edu.seu.openstack.domain.WholeEfficiencyGrade">
        SELECT id, grade, time
        FROM whole_efficiency_grade
        ORDER by time desc limit 1
    </select>

</mapper>